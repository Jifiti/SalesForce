            <preference preference-id="jifitisourcePageType">Checkout</preference>
            <preference preference-id="jifitistoreId">SFCC Location</preference>
            <preference preference-id="jifitiwindowBehavior">SameTab</preference>
            <preference preference-id="jifitipaymentTransactionType">Capture</preference>
            <preference preference-id="jifiticheckAccountStatusApi">applications/v2/CheckAccountStatus</preference>
            <preference preference-id="jifitiinitiateFlowApi">applications/v2/InitiateFlow</preference>
            <preference preference-id="jifitiPurchaseApi">purchases/v2/Authorize</preference>
            <preference preference-id="isJifitiEnabled">true</preference>
            <preference preference-id="enableCertInCart">true</preference>
            <preference preference-id="enableCertInPDP">true</preference>
            <preference preference-id="enableCertInPLP">true</preference>
            <preference preference-id="jifitiWidgetLibraryURL">https://toolbox-dev.jifiti.com/Widgets/1.0.3/jifiti-widget.min.js</preference>
            <preference preference-id="JifitiServiceLocation">credPayment.rest</preference>
        </all-instances>
        <development/>
        <staging/>
        <production/>
    </custom-preferences>
</preferences>
