<?xml version="1.0" encoding="UTF-8"?>
<preferences xmlns="http://www.demandware.com/xml/impex/preferences/2007-03-31">
    <custom-preferences>
        <all-instances>
            <preference preference-id="jifitiCompletionMethod">TransferCard</preference>
            <preference preference-id="jifitiSourceChannel">Online</preference>
            <preference preference-id="jifiticallBackURL">https://zyaf-001.sandbox.us01.dx.commercecloud.salesforce.com/on/demandware.store/Sites-RefArch-Site/en_US/CredPaymentController-CallBack</preference>
            <preference preference-id="credPaymentToken">cred payment token</preference>
            <preference preference-id="jifitilenderId">lenderId</preference>
            <preference preference-id="jifitimerchantId">merchantId</preference>
            <preference preference-id="jifitiproductTypes">
                <value>CO</value>
                <value>LC</value>
                <value>NaN</value>
                <value>ZeroPing</value>
                <value>Funded</value>
                <value>RevolvingInstore</value>
                <value>RevolvingOnline</value>
                <value>OTLInstore</value>
                <value>OTLOnline</value>
                <value>_3xCB</value>
                <value>SplitPayments</value>
