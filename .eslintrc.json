{
    "root": true,
    "extends": "airbnb-base/legacy",
    "env": {
        "browser": true,
        "commonjs": true,
        "jquery": true
    },
    "rules": {
        "import/no-unresolved": "off",
        "indent": ["error", 4, { "SwitchCase": 1, "VariableDeclarator": 1 }],
        "func-names": "off",
        "require-jsdoc": "error",
        "linebreak-style": 0,
        "valid-jsdoc": ["error", { "preferType": { "Boolean": "boolean", "Number": "number", "object": "Object", "String": "string" }, "requireReturn": false}],
        "vars-on-top": "off",
        "global-require": "off",
        "no-shadow": ["error", { "allow": ["err", "callback"]}],
        "max-len": "off",
        "no-continue": "off"
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "globals": {
        "dw": true,
        "empty" : true,
		"session": true,
		"request": true,
		"response": true
    }
}
